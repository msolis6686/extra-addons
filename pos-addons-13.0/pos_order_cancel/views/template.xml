<odoo>
    <template
        id="assets_backend"
        name="pos_category_multi_assets"
        inherit_id="point_of_sale.assets"
    >
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/pos_order_cancel/static/src/js/models.js"
            />
            <script
                type="text/javascript"
                src="/pos_order_cancel/static/src/js/widgets.js"
            />
        </xpath>
        <xpath expr="//link[@id='pos-stylesheet']" position="after">
            <link rel="stylesheet" href="/pos_order_cancel/static/src/css/pos.css" />
        </xpath>
    </template>
</odoo>
